{
  "name": "travelworld",
  "version": "1.0.0",
  "description": "travelApp",
  "main": "server.js",
  "proxy": "https://visittheworld.onrender.com",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "nodemon server.js",
    "minify-html1": "html-minifier-terser views/Screens/planner.html --collapse-whitespace --output=public/dist/planner.html",
    "minify-html2": "html-minifier-terser views/Screens/login.html --collapse-whitespace --output=public/dist/login.html",
    "minify-html3": "html-minifier-terser views/Screens/registration.html --collapse-whitespace --output=public/dist/registration.html",
    "optimize-css1": "postcss public/css/style.css --use autoprefixer cssnano postcss-pxtorem --output public/dist/style.css",
    "optimize-css2": "postcss public/css/responsive.css --use autoprefixer cssnano postcss-pxtorem --output public/dist/responsive.css",
    "optimize-css3": "postcss public/css/registration.css --use autoprefixer cssnano postcss-pxtorem --output public/dist/registration.css",
    "optimize-css4": "postcss public/css/planner.css --use autoprefixer cssnano postcss-pxtorem --output public/dist/planner.css",
    "optimize-css5": "postcss public/css/navbar.css --use autoprefixer cssnano postcss-pxtorem --output public/dist/navbar.css",
    "transpile-js1" : "babel public/js/main.js -o public/dist/main.js --presets=@babel/preset-env",
    "transpile-js2" : "babel public/js/global.js -o public/dist/global.js --presets=@babel/preset-env",
    "transpile-js3" : "babel public/js/planner.js -o public/dist/planner.js --presets=@babel/preset-env",
    "minify-js1" : "uglifyjs public/dist/main.js --compress -o public/dist/main.js",
    "minify-js2" : "uglifyjs public/dist/global.js --compress -o public/dist/global.js",
    "minify-js3" : "uglifyjs public/dist/planner.js --compress -o public/dist/planner.js",
    "build" : "npm run optimize-css1 && npm run optimize-css2 && npm run optimize-css3 && npm run optimize-css4 && npm run optimize-css5 && npm run transpile-js1 && npm run transpile-js2 && npm run transpile-js3 && npm run minify-js1 && npm run minify-js2 && npm run minify-js3"
    
  },
  "author": "amir azim",
  "license": "ISC",
  "dependencies": {
    "async_hooks": "^1.0.0",
    "body-parser": "^1.20.1",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.8",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "fs": "^0.0.1-security",
    "googleapis": "^111.0.0",
    "https": "^1.0.0",
    "mongodb": "^5.0.1",
    "mongoose": "^6.9.0",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.22",
    "openssl": "^2.0.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^7.1.2",
    "passport-local-mongoose-email": "^0.1.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.4",
    "@babel/preset-env": "^7.21.4",
    "autoprefixer": "^10.4.14",
    "browserify": "^17.0.0",
    "cssnano": "^6.0.0",
    "gulp": "^4.0.2",
    "html-minifier-terser": "^7.2.0",
    "postcss": "^8.4.21",
    "postcss-cli": "^10.1.0",
    "postcss-pxtorem": "^6.0.0",
    "uglify-js": "^3.17.4",
    "webpack": "^5.79.0",
    "webpack-cli": "^5.0.1"
  }
}
